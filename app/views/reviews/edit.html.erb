<div class="col-md-6 col-md-offset-3">

  <div class="edit-room-form">


  <h1>Editar reseña</h1>

<%= simple_form_for([@room, @review]) do |f| %>
    <div id="rating-form">
      <label>Calificación</label>
    </div>
    <%= f.input :comment %>
    <%= f.button :submit, :class => "btn-custom2"%>
<% end %>

  </div>
</div>

<script>
    $('#rating-form').raty({
        path: '/assets/',
        scoreName: 'review[rating]'
    });
</script>